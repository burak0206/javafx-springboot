<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<AnchorPane stylesheets="@../style/Styles.css" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zoho.recruit.cvuploader.controllers.FileChooseController">
<children>
   <Rectangle fill="DODGERBLUE" height="600.0" stroke="#ffffff8b" strokeType="INSIDE" width="900.0" />
   <Label fx:id="lblPage" layoutX="10.0" layoutY="40.0" text="Özgeçmişleri yüklenecek adayların listesini içeren dosyayı seçiniz." textFill="WHITE">
      <font>
         <Font size="20.0" />
      </font>
   </Label>
   <Button fx:id="btnDosyaSec" layoutX="10.0" layoutY="90.0" mnemonicParsing="false" onAction="#dosyaSec" prefHeight="30.0" prefWidth="130.0" styleClass="btnGreen" text="Dosya Seçiniz" textFill="WHITE">
      <font>
         <Font size="15.0" />
      </font>
      <effect>
         <DropShadow blurType="ONE_PASS_BOX" />
      </effect></Button>

   <Text fx:id="txtSelectedFile" layoutX="160.0" layoutY="110.0" text="Seçilen Dosya Yolu" >
      <font>
         <Font size="15.0" />
      </font>
   </Text>


   <Button fx:id="btnCVsProcess" layoutX="10.0" layoutY="150.0" mnemonicParsing="false" onAction="#candidatesProcess" prefHeight="30.0" prefWidth="130.0" styleClass="btnPro" text="Dosyaları Yükle" textFill="WHITE" disable="true">
      <font>
         <Font size="15.0" />
      </font>
      <effect>
         <DropShadow blurType="ONE_PASS_BOX" />
      </effect></Button>

</children>
<opaqueInsets>
   <Insets />
</opaqueInsets>
</AnchorPane>

