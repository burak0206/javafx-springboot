<AnchorPane stylesheets="@../style/Styles.css" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zoho.recruit.cvuploader.controllers.FileChooseController">
<children>
   <Rectangle fill="DODGERBLUE" height="600.0" stroke="#ffffff8b" strokeType="INSIDE" width="650.0" />
